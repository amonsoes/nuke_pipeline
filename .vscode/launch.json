{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "-logfolder",
                "-save_dir=./normalization/word_model",
                "-input=word",
                "-attention",
                "-bias",
                "-lowercase",
                "-bos",
                "-eos",
                "-brnn",
                "-batch_size=32",
                "-dropout=0.5",
                "-emb_size=100",
                "-end_epoch=1",
                "-layers=3",
                "-learning_rate_decay=0.05",
                "-lr=0.01",
                "-max_grad_norm=5",
                "-rnn_size=200",
                "-rnn_type=LSTM",
                "-tie_decoder_embeddings",
                "-share_embeddings",
                "-share_vocab",
                "-start_decay_after=15",
                "-teacher_forcing_ratio=0.6",
                "-max_train_decode_len=50",
                "-pretrained_emb=True"
            ],
        }
    ]
}